    "def train_batch(net, x, y, speech, loss_fn, optimizer):\n",
    "    \n",
    "    optimizer.zero_grad()\n",
    "    \n",
    "    out, _ = net(x, in_speech=speech)\n",
    "    \n",
    "    #loss = loss_fn(out[:, -1, :], y)\n",
    "    loss = loss_fn(out, y)\n",
    "    \n",
    "    loss.backward()\n",
    "    \n",
    "    optimizer.step()\n",
    "    \n",
    "    return float(loss.data)"
   ]
  }
